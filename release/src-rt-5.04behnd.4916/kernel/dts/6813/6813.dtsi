		#define GPON_GENERAL_CONFIG_ADDR    0x0 0x828E1000
		#define GPON_GENERAL_CONFIG_SIZE    0x0 0x10
		#define EPON_TOP_ADDR               0x0 0x828C0000
		#define EPON_TOP_SIZE               0x0 0x1C
