		#define EPON_XPCSR_SIZE	            0x0 0x124
		#define NGPON_RXGEN_ADDR            0x0 0x828E4000
		#define NGPON_RXGEN_SIZE            0x0 0x80
		#include "../ip/bcm_wantype_detect.dtsi"
	};

