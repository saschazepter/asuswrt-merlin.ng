		#define WIFI_ADDR			0x86000000
		#define WIFI_SIZE			0x01000000
		#define WIFI_DEVID			0x6756
		#define WIFI_CCM_SPI		50
		#define WIFI_D11MAC_SPI		51
		#define WIFI_M2MDMA_SPI		52
		#define WIFI_WDRST_SPI		53
